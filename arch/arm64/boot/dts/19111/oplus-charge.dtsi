	pinctrl-3 = <&ship_sleep>;
	pinctrl-4 = <&chargerid_adc_default>;
	pinctrl-5 = <&uart_bias_disable>;
	pinctrl-6 = <&uart_pull_down>;
	pinctrl-7 = <&dischg_enable>;
	pinctrl-8 = <&dischg_disable>;
