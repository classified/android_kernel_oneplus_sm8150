&soc {

	//#ifdef OPLUS_FEATURE_CONNFCSOFT
	nfc_chipset {
		compatible = "oplus-nfc-chipset";
		chipset-18501 = "NQ330";
	};
	//#endif /* OPLUS_FEATURE_CONNFCSOFT */

	gpio_keys {
		compatible = "gpio-keys";
		label = "gpio-keys";

		pinctrl-names = "default";
		/*ifdef VENDOR_EDIT*/
		pinctrl-0 = <&key_vol_up_default &key_vol_down_default>;
		//#endif

		vol_up {
			label = "volume_up";
			gpios = <&pm8150_gpios 6 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEUP>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};

		/*ifdef VENDOR_EDIT*/
		vol_down {
			label = "volume_down";
			gpios = <&pm8150_gpios 7 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEDOWN>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};
		/*endif*/
	};

	//#ifdef OPLUS_FEATURE_SENSOR_SMEM
	/*enum sensor_id { OPLUS_ACCEL,OPLUS_GYRO,OPLUS_MAG,OPLUS_LIGHT,OPLUS_PROXIMITY,OPLUS_SAR,OPLUS_CT,OPLUS_BAROMETER,SENSORS_NUM};*/
        sensor_fb: sensor_fb {
            compatible = "oplus,sensor-feedback";
            master-id  = <3>;
            smem-id    = <608>;
        };
	oplus_sensor {
		compatible = "oplus,sensor-devinfo";
		als-row-coe = <540>;
		/*enum {LSM6DSM = 0x01, BMI160 = 0x02, LSM6DS3_C = 0x04};*/
		gsensor@0 {
			sensor-name = <0x01>;//LSM6DSM
			sensor-direction = <6>;
			sensor-index = <0>;//source num
			sensor-type = <0>;//OPLUS_ACCEL
		};
		/* enum {AKM0991X = 0x01, MMC5603 = 0x02};*/
		msensor@0 {
			sensor-name = <0x02>;//MMC5603
			sensor-direction = <0>;
			sensor-index = <0>;
			sensor-type = <2>;//OPLUS_MAG
			parameter-number= <18>;
			//sensor-parameter = <2>;//0-useless;other- to differ special parameter
			soft-mag-parameter = <9986 0 190 1 484 1 307 1 10630 0 31 1 452 1 145 1 11978 0>;//e00 sign e01 sign ...
		};
		/*enum {SX9324 = 0X01}*/
		ssensor@0{
			sensor-name = <0x01>;//SX9324
			sensor-index = <0>;
			sensor-type = <5>;//OPLUS_SAR
			parameter-number = <24>;
			sensor-reg = <0x10 0x17 0x20 0x96 0x23 0x01 0x24 0x5f 0x26 0x01 0x27 0x5f 0x28 0x1d 0x29 0x2d 0x2a 0x3d 0x2d 0x08 0x30 0x09 0x31 0x09>;
			feature-number = <6>;
			sensor-feature-reg = <0x36 0x3c 0x37 0x3f 0x11 0x27>;
		};
		/*virtual sensor
		enum sensor_algo_id {
			OPLUS_PICKUP_DETECT,
			OPLUS_LUX_AOD,
			OPLUS_TP_GESTURE,
			OPLUS_FP_DISPLAY,
			OPLUS_FREE_FALL,
			OPLUS_CAMERA_PROTECT,
			SENSOR_ALGO_NUM
		}*/
		pickup_detect {
			sensor-type = <0>;//OPLUS_PICKUP_DETECT
			is-virtual-sensor;
			is-need-prox = <0>;//0 - disable; 1 - enable
			prox-type = <1>;//prox_type: 0 - proximity; 1 - proximity_fake
		};
		lux_aod {
			sensor-type = <1>; //OPLUS_LUX_AOD
			is-virtual-sensor;
			thrd-low = <12>;
			thrd-high = <50>;
			als-type = <1>; // 0 - wise_light; 1 - ambient_light
		};
		fp_display {
			sensor-type = <3>; //OPLUS_FP_DISPLAY
			is-virtual-sensor;
			prox-type = <1>; // 0 - proximity; 1 - proximity_fake
		};
		free_fall {
			sensor-type = <4>; //OPLUS_FREE_FALL
			is-virtual-sensor;
		};
		tp_gesture {
			sensor-type = <2>; //OPLUS_TP_GESTURE
			is-virtual-sensor;
		};
		camera_protect {
			sensor-type = <5>; //OPLUS_CAMERA_PROTECT
			is-virtual-sensor;
			layout_offset_x = <270>;
			layout_offset_y = <361>;
			init_pin = <163>;
			is_chip_pin = <1>;
		};
	};
	//#endif /*OPLUS_FEATURE_SENSOR_SMEM*/

