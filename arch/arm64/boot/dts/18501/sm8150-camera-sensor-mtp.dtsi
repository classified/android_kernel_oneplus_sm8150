/* #ifdef VENDOR_EDIT */
&tlmm {

	cam_sensor_rear2_active: cam_sensor_rear2_active {
		/* RESET REAR2 */
		mux {
			pins = "gpio27", "gpio148", "gpio166", "gpio165";
			function = "gpio";
		};

		config {
			pins = "gpio27", "gpio148", "gpio166", "gpio165";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
			output-high;
		};
	};

	cam_sensor_rear2_suspend: cam_sensor_rear2_suspend {
		/* RESET REAR2 */
		mux {
			pins = "gpio27", "gpio148", "gpio166", "gpio165";
			function = "gpio";
		};

		config {
			pins = "gpio27", "gpio148", "gpio166", "gpio165";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};
	cam_sensor_reset2_active: cam_sensor_reset2_active {
		/* RESET REAR2 */
		mux {
			pins = "gpio23";
			function = "gpio";
		};

		config {
			pins = "gpio23";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_reset2_suspend: cam_sensor_reset2_suspend {
		/* RESET REAR2 */
		mux {
			pins = "gpio23";
			function = "gpio";
		};

		config {
			pins = "gpio23";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};
};
/* #endif */

&soc {
	#address-cells = <1>;
	#size-cells = <1>;

	led_flash_rear: qcom,camera-flash@0 {
		cell-index = <0>;
		reg = <0x00 0x00>;
		compatible = "qcom,camera-flash";
		qcom,flash-name = "pmic";
		flash-source = <&pm8150l_flash0 &pm8150l_flash1>;
		torch-source = <&pm8150l_torch0 &pm8150l_torch1>;
		switch-source = <&pm8150l_switch3>;
		status = "ok";
	};
	led_flash_rear_aux: qcom,camera-flash@1 {
		cell-index = <1>;
		reg = <0x01 0x00>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8150l_flash0 &pm8150l_flash1>;
		torch-source = <&pm8150l_torch0 &pm8150l_torch1>;
		switch-source = <&pm8150l_switch3>;
		status = "ok";
	};
	led_flash_rear_aux2: qcom,camera-flash@3 {
		cell-index = <3>;
		reg = <0x03 0x00>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8150l_flash0 &pm8150l_flash1>;
		torch-source = <&pm8150l_torch0 &pm8150l_torch1>;
		switch-source = <&pm8150l_switch3>;
		status = "ok";
	};

	af_ldo: gpio-regulator@7 {
		compatible = "regulator-fixed";
		reg = <0x00 0x00>;
		regulator-name = "af_ldo";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-enable-ramp-delay = <135>;
		enable-active-high;
		gpio = <&tlmm 11 0>;
		pinctrl-names = "default";
		//pinctrl-0 = <&cam_sensor_dvdd_en>;
		vin-supply = <&pm8150l_l1>;
	};
	led_flash_front: qcom,camera-flash@2 {
		cell-index = <2>;
		reg = <0x02 0x00>;
		compatible = "qcom,camera-flash";
		/*#ifndef VENDOR_EDIT*/
		/*
		wled-flash-support;
		flash-source = <&wled_flash>;
		torch-source = <&wled_torch>;
		switch-source = <&wled_switch>;
		*/
		/*#else*/
		flash-source = <&pm8150l_flash2>;
		torch-source = <&pm8150l_torch2>;
		switch-source = <&pm8150l_switch2>;
		/*#endif*/
		status = "ok";
	};
