		ldo1-config = <3300000 3300000 100000>;  /*206B1 use 100000 for ldo-config */


		goodix,gpio_irq = <&tlmm 90 0x1>;
		goodix,gpio_reset = <&tlmm 92 0x0>;  /*206B1 use 92 for reset gpio */
		pinctrl-names = "default";
		pinctrl-0 = <&gpio_goodix_irq_default>;

		power-num = <1> ;
